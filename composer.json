{
  "name": "yuncms/yuncms-supervisor",
  "description": "Supervisor manager module for YUNCMS",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "supervisor",
    "supervisor-manager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "AlexyAV",
      "email": "avpretty@gmail.com"
    },
    {
      "name": "XuTongle",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "~2.0",
    "zendframework/zend-xmlrpc": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.*",
    "mikey179/vfsStream": "~1"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\supervisor\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "supervisor",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/supervisor/messages"
      },
      "cron": {
      },
      "migrationNamespace": "yuncms\\supervisor\\migrations",
      "backend": {
        "class": "yuncms\\supervisor\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
